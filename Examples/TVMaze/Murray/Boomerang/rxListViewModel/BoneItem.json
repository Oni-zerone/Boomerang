{
    "replacements" : [
        {
            "source" : "ViewModelFactoryImplementation.replacement.swift",
            "destination" : "Sources\/{{ target || \"App\" }}\/Factory\/SceneViewModelFactory.swift",
            "placeholder": "//MURRAY IMPLEMENTATION PLACEHOLDER"
        },
        {
            "source" : "ViewModelFactoryDeclaration.replacement.swift",
            "destination" : "Sources\/{{ target || \"App\" }}\/Factory\/SceneViewModelFactory.swift",
            "placeholder": "\/\/MURRAY DECLARATION PLACEHOLDER"
        }
    ],
    "paths" : [
        
        {
            "from" : "ViewModel.template.swift",
            "to" : "{{ target || \"App\" }}\/ViewControllers/{{ name | firstUppercase }}/{{ name | firstUppercase}}ViewModel.swift"
        }
        
    ],
    "parameters" : [
        {
            "name": "name",
            "isRequired": true
        },
        {
            "name": "target",
            "isRequired": true
        }
    ],
    "name" : "rxListViewModel",
    "plugins" : {
        "xcode": {
            "targets": ["{{ target || \"App\" }}"]
        }
    }
}
