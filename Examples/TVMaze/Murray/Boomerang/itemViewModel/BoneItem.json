{
    "replacements" : [
        {
            "source" : "ViewModelFactoryImplementation.replacement.swift",
            "destination" : "Sources\/{{ target || \"App\" }}\/Factory\/ItemViewModelFactory.swift",
            "placeholder": "//MURRAY IMPLEMENTATION PLACEHOLDER"
        },
        {
            "source" : "ViewModelFactoryDeclaration.replacement.swift",
            "destination" : "Sources\/{{ target || \"App\" }}\/Factory\/ItemViewModelFactory.swift",
            "placeholder": "\/\/MURRAY DECLARATION PLACEHOLDER"
        }
    ],
    "paths" : [
        
        {
            "from" : "ItemViewModel.template.swift",
            "to" : "{{ target || \"App\" }}\/Views/{{ name | firstUppercase }}/{{ name | firstUppercase}}ItemViewModel.swift"
        }
        
    ],
    "parameters" : [
        {
            "name": "name",
            "isRequired": true
        },
        {
            "name": "target",
            "isRequired": true
        }
    ],
    "name" : "itemViewModel",
    "plugins" : {
        "xcode": {
            "targets": ["{{ target || \"App\" }}"]
        }
    }
}
